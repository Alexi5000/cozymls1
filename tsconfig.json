{
  "files": [],
  "references": [
    { "path": "./tsconfig.app.json" },
    { "path": "./tsconfig.node.json" }
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@/app": ["./src/app"],
      "@/pages": ["./src/pages"],
      "@/widgets": ["./src/widgets"],
      "@/features": ["./src/features"],
      "@/entities": ["./src/entities"],
      "@/shared": ["./src/shared"],
      "@/processes": ["./src/processes"]
    },
    "noImplicitAny": false,
    "noUnusedParameters": false,
    "skipLibCheck": true,
    "allowJs": true,
    "noUnusedLocals": false,
    "strictNullChecks": false
  }
}
